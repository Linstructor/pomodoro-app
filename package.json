{
  "name": "pomodoro",
  "version": "1.0.0",
  "description": "Pomodoro App",
  "main": "index.js",
  "scripts": {
    "start": "rm -r -f ./public/final && ./node_modules/.bin/parcel build ./public/index.html -d ./public/final -o index.html --target=electron --public-url '.' && ./node_modules/.bin/electron ./index.js dev",
    "start-elec": "./node_modules/.bin/electron ./index.js dev",
    "dev-front": "./node_modules/.bin/parcel ./public/index.html -d ./public/final -o index.html --target=electron --public-url '.'",
    "build-front": "./node_modules/.bin/parcel build ./public/index.html -d ./public/final -o index.html --target=electron --public-url '.'",
    "build-app": "rm -rf ./dist/* && npm run build-front && electron-packager . Pomodoro --platform=darwin --icon './public/assets/img/icon_osx.icns' --out ./dist --overwrite"
  },
  "author": "Tristan Morisot <tristan.morisot@viacesi.fr>",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.1.4",
    "vue": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@vue/component-compiler-utils": "^2.6.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "css-loader": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^4.2.0",
    "electron-installer-dmg": "^2.0.0",
    "electron-packager": "^13.1.1",
    "parcel-bundler": "^1.12.3",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.2",
    "vue-class-component": "^7.1.0",
    "vue-devtools": "^5.0.0-beta.1",
    "vue-hot-reload-api": "^2.3.3",
    "vue-loader": "^15.7.0",
    "vue-property-decorator": "latest",
    "vue-template-compiler": "^2.6.10",
    "vuex-class": "^0.3.2",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4"
  }
}
